<p><%= @item.name %></p>
<a href="<%= @item.link %>
">Go to product</a>
<p>$<%= @item.price %></p>
<img src="<%= @item.image %>" alt="">
<p>Rating: <%= @item.rating %></p>
 <% @item.wishlists.each do |w| %>
<p> wish : <%= w.name %></p>
<% end %>

<% if user_signed_in? && current_user == @item.user %>
<%= link_to 'Edit', "/items/#{@item.id}/edit" %> 
<%= link_to 'Delete', "/items/#{@item.id}", method: "delete" %> 
<% end %>